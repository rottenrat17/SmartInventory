@model IEnumerable<SmartInventoryManagement.Models.Order>
<h2>Track Orders</h2>
<table class="table">
    <tr>
        <th>Order ID</th>
        <th>Customer</th>
        <th>Date</th>
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.Id</td>
            <td>@order.CustomerName</td>
            <td>@order.OrderDate</td>
        </tr>
    }
</table>