@model List<string>

<h2>Product List</h2>

<div class="product-container">
    @foreach (var product in Model)
    {
        <div class="product-card">
            <h3>@product</h3>
        </div>
    }
</div>

<style>
    .product-container {
        display: flex;
        flex-wrap: wrap;
        gap: 15px;
        margin-top: 20px;
    }

    .product-card {
        background-color: #f8f9fa;
        padding: 15px;
        border-radius: 8px;
        box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.1);
        text-align: center;
        width: 200px;
    }

    .product-card h3 {
        margin: 0;
        font-size: 18px;
    }
</style>